.container {
  margin: 0;
  min-height: 100vh;
}
.container .header {
  height: 200px;
  width: 100%;
  background: #259D8F;
  padding: 20px;
  box-sizing: border-box;
}
.container .header .swiper {
  width: 100%;
  height: 100%;
}
.container .header .swiper .slide-image {
  width: 100%;
  height: 100%;
  border-radius: 6px;
}
.container .content {
  padding: 12px;
  box-sizing: border-box;
}
.container .content .card {
  display: flex;
  box-sizing: border-box;
}
.container .content .card .item {
  width: calc(50% - 6px);
  height: 330px;
}
.container .content .card .item .high {
  width: 100%;
  height: 200px;
  background: #EEFFF9;
  border: 1px solid #259D8F;
  border-radius: 6px;
  color: #259D8F;
  font-size: 18px;
  font-weight: 600;
  position: relative;
}
.container .content .card .item .high .slide-image {
  width: 130px;
  height: 160px;
  position: absolute;
  bottom: 0;
}
.container .content .card .item .high .text {
  position: absolute;
  top: 20px;
}
.container .content .card .item .high .right-text {
  right: 20px;
}
.container .content .card .item .high .left-text {
  left: 20px;
}
.container .content .card .item .high .right-img {
  right: 0;
}
.container .content .card .item .high .left-img {
  left: 0;
}
.container .content .card .item .low {
  background: #FFFAED;
  width: 100%;
  height: 120px;
  border: 1px solid #F4BD3A;
  border-radius: 6px;
  color: #F4BD3A;
  font-size: 18px;
  font-weight: 600;
  position: relative;
}
.container .content .card .item .low .slide-image {
  width: 126px;
  height: 117px;
  position: absolute;
  bottom: -10px;
}
.container .content .card .item .low .text {
  position: absolute;
  top: 20px;
}
.container .content .card .item .low .right-img {
  right: -10px;
}
.container .content .card .item .low .left-img {
  left: -10px;
}
.container .content .card .item .low .right-text {
  right: 20px;
}
.container .content .card .item .low .left-text {
  left: 20px;
}
.container .content .card .item .top {
  margin-bottom: 10px;
}
.container .content .card .item .bottom {
  margin-top: 10px;
}
.container .content .card .left {
  margin-right: 12px;
}
.container .content .list {
  margin-bottom: 80px;
}
.container .content .list .title {
  margin: 20px 0;
  display: flex;
  justify-content: space-between;
}
.container .content .list .title .left {
  font-weight: 600;
  color: #000000;
  font-size: 16px;
}
.container .content .list .title .right {
  color: #259D8F;
  display: flex;
  align-items: center;
}
